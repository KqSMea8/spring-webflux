<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xmlns:springwebflux="http://www.newlandframework.com/springwebflux"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.newlandframework.com/springwebflux/springwebflux.xsd"
       default-autowire="byName">

        <springwebflux:service interfaceName="" ref=""></springwebflux:service>
        <springwebflux:reference interfaceName="" ipAddr="" protocol=""></springwebflux:reference>
        <springwebflux:registry ipAddr="" echoApiPort="" protocol=""></springwebflux:registry>

        <!-- 注入spring boot无法扫描到的bean. -->
        <bean id="noScanService" class="cannotscan.service.CannotScanService"></bean>






</beans>